{
  "name": "notenick",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "b": "pnpm --filter @notenick/backend",
    "w": "pnpm --filter @notenick/webapp",
    "dev": "pnpm -r --parallel dev",
    "types": "concurrently \"pnpm b types\" \"pnpm w types\"",
    "prettify": "pnpm -r --parallel prettify"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}